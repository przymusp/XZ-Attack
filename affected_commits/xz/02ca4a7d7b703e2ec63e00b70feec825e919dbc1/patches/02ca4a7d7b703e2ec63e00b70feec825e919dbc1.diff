 msgid "Total number of blocks in the stream(s)"
 msgstr "Numărul total de blocuri din fluxuri"
 
 #. type: IP
 #: ../src/xz/xz.1:2169 ../src/xz/xz.1:2206 ../src/xz/xz.1:2233
 #: ../src/xz/xz.1:2313 ../src/xz/xz.1:2329 ../src/xz/xz.1:2428
