 msgid "I<dict> * 6.5 (if I<dict> E<gt> 32 MiB)"
 msgstr "I<dict> * 6,5 (falls I<dict> E<gt> 32 MiB ist)"
 
 #. type: TP
 #: ../src/xz/xz.1:1674
 #, no-wrap
 msgid "B<bt2>"
 msgstr "B<bt2>"
 
