 msgid "Compressed size of the file"
 msgstr "Dimensiunea comprimată a fișierului"
 
 #. type: IP
 #: ../src/xz/xz.1:2171 ../src/xz/xz.1:2208 ../src/xz/xz.1:2235
 #: ../src/xz/xz.1:2331 ../src/xz/xz.1:2440
