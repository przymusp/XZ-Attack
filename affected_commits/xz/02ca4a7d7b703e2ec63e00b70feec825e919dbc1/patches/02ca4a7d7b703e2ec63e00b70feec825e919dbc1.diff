 msgstr "B<hc3>"
 
 #. type: Plain text
 #: ../src/xz/xz.1:1639
 msgid "Hash Chain with 2- and 3-byte hashing"
 msgstr "Hash-Kette mit 2- und 3-Byte-Hashing"
 
